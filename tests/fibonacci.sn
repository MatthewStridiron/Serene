integer x;

function fib(integer x) -> integer {
if(x <= 1) {
return x;
}
return fib(x-1) + fib(x-2);
}

function main() -> integer {
printstring("RUNNING FIBONACCI.SN");
x = 5;
print(fib(5));
print(fib(12));
print(fib(24));
print(fib(25));
printstring("FIBONACCI WORKS SUCCESSFULLY");
printstring("----");
return 1;
}
